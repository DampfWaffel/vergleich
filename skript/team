command /testb:
    trigger:
        loop all players:
            if loop-player's group is "leben":
                add 1 to {_num::leben}
            else if loop-player's group is "tot":
                add 1 to {_num::tot}
            set action bar of loop-player to "Spieler am leben: &a&l%{_num::leben}% &fSpieler tot: &c&l%{_num::tot}%"
