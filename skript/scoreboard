command /updatescoreboard:
    trigger:
        set {_inhaberpermission} to skript-yaml value "Rang Permissions.Permissions Inhaber" of "plugins/Lobby System/scoreboard.yml"
        set {_adminpermission} to skript-yaml value "Rang Permissions.Permissions Admin" of "plugins/Lobby System/scoreboard.yml"
        set {_gamedesignerpermission} to skript-yaml value "Rang Permissions.Permissions Game Designer" of "plugins/Lobby System/scoreboard.yml"
        set {_headdevpermission} to skript-yaml value "Rang Permissions.Permissions Head Developer" of "plugins/Lobby System/scoreboard.yml"
        set {_devpermission} to skript-yaml value "Rang Permissions.Permissions Developer" of "plugins/Lobby System/scoreboard.yml"
        set {_testdevpermission} to skript-yaml value "Rang Permissions.Permissions Test Developer" of "plugins/Lobby System/scoreboard.yml"
        set {_srmodpermission} to skript-yaml value "Rang Permissions.Permissions Sr Moderator" of "plugins/Lobby System/scoreboard.yml"
        set {_modpermission} to skript-yaml value "Rang Permissions.Permissions Moderator" of "plugins/Lobby System/scoreboard.yml"
        set {_testmodpermission} to skript-yaml value "Rang Permissions.Permissions Test Moderator" of "plugins/Lobby System/scoreboard.yml"
        set {_headarchipermission} to skript-yaml value "Rang Permissions.Permissions Head Architekt" of "plugins/Lobby System/scoreboard.yml"
        set {_archipermission} to skript-yaml value "Rang Permissions.Permissions Architekt" of "plugins/Lobby System/scoreboard.yml"
        set {_testarchipermission} to skript-yaml value "Rang Permissions.Permissions Test Architekt" of "plugins/Lobby System/scoreboard.yml"
        set {_youtuberpermission} to skript-yaml value "Rang Permissions.Permissions Youtuber" of "plugins/Lobby System/scoreboard.yml"
        set {_streamerpermission} to skript-yaml value "Rang Permissions.Permissions Streamer" of "plugins/Lobby System/scoreboard.yml"
        set {_sponsorpermission} to skript-yaml value "Rang Permissions.Permissions Sponsor" of "plugins/Lobby System/scoreboard.yml"
        set {_freundpermission} to skript-yaml value "Rang Permissions.Permissions Freund" of "plugins/Lobby System/scoreboard.yml"
        set {_mvppluspermission} to skript-yaml value "Rang Permissions.Permissions MVP +" of "plugins/Lobby System/scoreboard.yml"
        set {_mvppermission} to skript-yaml value "Rang Permissions.Permissions MVP" of "plugins/Lobby System/scoreboard.yml"
        set {_primepermission} to skript-yaml value "Rang Permissions.Permissions Prime" of "plugins/Lobby System/scoreboard.yml"
        set {_vippluspermission} to skript-yaml value "Rang Permissions.Permissions VIP +" of "plugins/Lobby System/scoreboard.yml"
        set {_vippermission} to skript-yaml value "Rang Permissions.Permissions VIP" of "plugins/Lobby System/scoreboard.yml"
        set {_inhaberdesign} to skript-yaml value "Rang Design.Design Inhaber" of "plugins/Lobby System/scoreboard.yml"
        set {_admindesign} to skript-yaml value "Rang Design.Design Admin" of "plugins/Lobby System/scoreboard.yml"
        set {_gamedesignerdesign} to skript-yaml value "Rang Design.Design Game Designer" of "plugins/Lobby System/scoreboard.yml"
        set {_headdevdesign} to skript-yaml value "Rang Design.Design Head Developer" of "plugins/Lobby System/scoreboard.yml"
        set {_devdesign} to skript-yaml value "Rang Design.Design Developer" of "plugins/Lobby System/scoreboard.yml"
        set {_testdevdesign} to skript-yaml value "Rang Design.Design Test Developer" of "plugins/Lobby System/scoreboard.yml"
        set {_srmoddesign} to skript-yaml value "Rang Design.Design Sr Moderator" of "plugins/Lobby System/scoreboard.yml"
        set {_moddesign} to skript-yaml value "Rang Design.Design Moderator" of "plugins/Lobby System/scoreboard.yml"
        set {_testmoddesign} to skript-yaml value "Rang Design.Design Test Moderator" of "plugins/Lobby System/scoreboard.yml"
        set {_headarchidesign} to skript-yaml value "Rang Design.Design Head Architekt" of "plugins/Lobby System/scoreboard.yml"
        set {_archidesign} to skript-yaml value "Rang Design.Design Architekt" of "plugins/Lobby System/scoreboard.yml"
        set {_testarchidesign} to skript-yaml value "Rang Design.Design Test Architekt" of "plugins/Lobby System/scoreboard.yml"
        set {_youtuberdesign} to skript-yaml value "Rang Design.Design Youtuber" of "plugins/Lobby System/scoreboard.yml"
        set {_streamerdesign} to skript-yaml value "Rang Design.Design Streamer" of "plugins/Lobby System/scoreboard.yml"
        set {_sponsordesign} to skript-yaml value "Rang Design.Design Sponsor" of "plugins/Lobby System/scoreboard.yml"
        set {_freunddesign} to skript-yaml value "Rang Design.Design Freund" of "plugins/Lobby System/scoreboard.yml"
        set {_mvpplusdesign} to skript-yaml value "Rang Design.Design MVP +" of "plugins/Lobby System/scoreboard.yml"
        set {_mvpdesign} to skript-yaml value "Rang Design.Design MVP" of "plugins/Lobby System/scoreboard.yml"
        set {_primedesign} to skript-yaml value "Rang Design.Design Prime" of "plugins/Lobby System/scoreboard.yml"
        set {_vipplusdesign} to skript-yaml value "Rang Design.Design VIP +" of "plugins/Lobby System/scoreboard.yml"
        set {_vipdesign} to skript-yaml value "Rang Design.Design VIP" of "plugins/Lobby System/scoreboard.yml"
        set {_spielerdesign} to skript-yaml value "Rang Design.Design Spieler" of "plugins/Lobby System/scoreboard.yml"
        set {_titlescoreboard} to skript-yaml value "Scoreboard.Title" of "plugins/Lobby System/scoreboard.yml"
        set {_zeile1scoreboard} to skript-yaml value "Scoreboard.Zeile 1" of "plugins/Lobby System/scoreboard.yml"
        set {_zeile2scoreboard} to skript-yaml value "Scoreboard.Zeile 2" of "plugins/Lobby System/scoreboard.yml"
        set {_zeile4scoreboard} to skript-yaml value "Scoreboard.Zeile 4" of "plugins/Lobby System/scoreboard.yml"
        set {_zeile5scoreboard} to skript-yaml value "Scoreboard.Zeile 5" of "plugins/Lobby System/scoreboard.yml"
        set {_zeile6scoreboard} to skript-yaml value "Scoreboard.Zeile 6" of "plugins/Lobby System/scoreboard.yml"
        set {_zeile7scoreboard} to skript-yaml value "Scoreboard.Zeile 7" of "plugins/Lobby System/scoreboard.yml"
        set {_zeile8scoreboard} to skript-yaml value "Scoreboard.Zeile 8" of "plugins/Lobby System/scoreboard.yml"
        set {_zeile9scoreboard} to skript-yaml value "Scoreboard.Zeile 9" of "plugins/Lobby System/scoreboard.yml"
        set {_zeile10scoreboard} to skript-yaml value "Scoreboard.Zeile 10" of "plugins/Lobby System/scoreboard.yml"
        set {_zeile11scoreboard} to skript-yaml value "Scoreboard.Zeile 11" of "plugins/Lobby System/scoreboard.yml"
        set {_zeile12scoreboard} to skript-yaml value "Scoreboard.Zeile 12" of "plugins/Lobby System/scoreboard.yml"
        set {_zeile13scoreboard} to skript-yaml value "Scoreboard.Zeile 13" of "plugins/Lobby System/scoreboard.yml"
        set {_zeile14scoreboard} to skript-yaml value "Scoreboard.Zeile 14" of "plugins/Lobby System/scoreboard.yml"
        set {_zeile15scoreboard} to skript-yaml value "Scoreboard.Zeile 15" of "plugins/Lobby System/scoreboard.yml"
        replace all "[coins]" in {_zeile9scoreboard} with "%{money.%player%}%"
        replace all "[stunde]" in {_zeile6scoreboard} with "%{stunde.%player%}%"
        replace all "[minute]" in {_zeile6scoreboard} with "%{minute.%player%}%"
		if player does not have a skoreboard:
			setup skoreboard for player
        if player has permission "%{_inhaberpermission}%":
			set slot 3 of skoreboard player to "%{_inhaberdesign}%"
        else if player has permission "%{_adminpermission}%":
			set slot 3 of skoreboard player to "%{_admindesign}%"
        else if player has permission "%{_gamedesignerpermission}%":
			set slot 3 of skoreboard player to "%{_gamedesignerdesign}%"
        else if player has permission "%{_headdevpermission}%":
			set slot 3 of skoreboard player to "%{_headdevdesign}%"
        else if player has permission "%{_devpermission}%":
			set slot 3 of skoreboard player to "%{_devdesign}%"
        else if player has permission "%{_testdevpermission}%":
			set slot 3 of skoreboard player to "%{_testdevdesign}%"
        else if player has permission "%{_srmodpermission}%":
			set slot 3 of skoreboard player to "%{_srmoddesign}%"
        else if player has permission "%{_modpermission}%":
			set slot 3 of skoreboard player to "%{_moddesign}%"
        else if player has permission "%{_testmodpermission}%":
			set slot 3 of skoreboard player to "%{_testmoddesign}%"
        else if player has permission "%{_headarchipermission}%":
			set slot 3 of skoreboard player to "%{_headarchidesign}%"
        else if player has permission "%{_archipermission}%":
			set slot 3 of skoreboard player to "%{_archidesign}%"
        else if player has permission "%{_testarchipermission}%":
			set slot 3 of skoreboard player to "%{_testarchidesign}%"
        else if player has permission "%{_youtuberpermission}%":
			set slot 3 of skoreboard player to "%{_youtuberdesign}%"
        else if player has permission "%{_streamerpermission}%":
			set slot 3 of skoreboard player to "%{_streamerdesign}%"
        else if player has permission "%{_sponsorpermission}%":
			set slot 3 of skoreboard player to "%{_sponsordesign}%"
        else if player has permission "%{_freundpermission}%":
			set slot 3 of skoreboard player to "%{_freunddesign}%"
        else if player has permission "%{_mvppluspermission}%":
			set slot 3 of skoreboard player to "%{_mvpplusdesign}%"
        else if player has permission "%{_mvppermission}%":
			set slot 3 of skoreboard player to "%{_mvpdesign}%"
        else if player has permission "%{_primepermission}%":
			set slot 3 of skoreboard player to "%{_primedesign}%"
        else if player has permission "%{_vippluspermission}%":
			set slot 3 of skoreboard player to "%{_vipplusdesign}%"
        else if player has permission "%{_vippermission}%":
			set slot 3 of skoreboard player to "%{_vipdesign}%"
        else:
			set slot 3 of skoreboard player to "%{_spielerdesign}%"
		set title of skoreboard player to "%{_titlescoreboard}%"
		set slot 1 of skoreboard player to "%{_zeile1scoreboard}%"
		set slot 2 of skoreboard player to "%{_zeile2scoreboard}%"
		#Design Line
		set slot 4 of skoreboard player to "%{_zeile4scoreboard}%"
		set slot 5 of skoreboard player to "%{_zeile5scoreboard}%"
		set slot 6 of skoreboard player to "%{_zeile6scoreboard}%"
		set slot 7 of skoreboard player to "%{_zeile7scoreboard}%"
		set slot 8 of skoreboard player to "%{_zeile8scoreboard}%"
		set slot 9 of skoreboard player to "%{_zeile9scoreboard}%"
		set slot 10 of skoreboard player to "%{_zeile10scoreboard}%"
		set slot 11 of skoreboard player to "%{_zeile11scoreboard}%"
		set slot 12 of skoreboard player to "%{_zeile12scoreboard}%"
		set slot 13 of skoreboard player to "%{_zeile13scoreboard}%"
		set slot 14 of skoreboard player to "%{_zeile14scoreboard}%"
		set slot 15 of skoreboard player to "%{_zeile15scoreboard}%"
